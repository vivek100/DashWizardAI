{
  "dependencies": [
    "langgraph>=0.2.20",
    "langgraph-sdk>=0.1.0",
    "langgraph-cli[inmem]>=0.3.0",
    "langchain>=0.3.0",
    "langchain-core>=0.3.0",
    "langchain-openai>=0.2.0",
    "pandas", 
    "duckdb",
    "python-dotenv",
    "fastapi",
    "uvicorn",
    "supabase",
    "pyjwt",
    "psycopg2-binary",
    "redis",
    "numpy",
    "textblob",
    "scikit-learn",
    "pytest",
    "pytest-asyncio",
    "black",
    "python-multipart",
    "."
  ],
  "graphs": {
    "dashboard_agent": "agent.agent:agent"
  },
  "http": {
    "app": "agent/custom_app.py:app"
  },
  "env": "./.env",
  "cors": {
    "allow_origins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "http://localhost:8080",
      "https://your-frontend-app.onrender.com",
      "https://*.onrender.com",
      "https://your-frontend-app.vercel.app",
      "https://*.vercel.app",
      "*"
    ],
    "allow_credentials": true,
    "allow_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allow_headers": ["*"]
  }
} 